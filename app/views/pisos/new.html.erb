 <div class="new">
  <%= form_with model: @piso do |f| %>
    <h2><%= f.label :nome %></h2>
    <%= f.text_field :nome %>
    <h2><%= f.label :tamanho %></h2>
    <%= f.text_field :tamanho %>
    <h2><%= f.label :valor %></h2>
    <%= f.text_field :valor %>
    <h3><%= f.submit "Criar"%></h3>
  <% end %>
</div>
